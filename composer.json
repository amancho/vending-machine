{
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VendingMachine\\Tests\\Unit\\": "tests/Unit/VendingMachine",
      "VendingMachine\\Tests\\Integration\\": "tests/Integration/VendingMachine"
    }
  },
  "require": {
    "php": "^8.2",
    "symfony/console": "^6.4",
    "ext-pdo": "*",
    "symfony/messenger": "6.0.*",
    "symfony/yaml": "6.0.*",
    "symfony/framework-bundle": "6.0.*",
    "symfony/runtime": "^7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "infection/infection": "^0.26.6",
    "phpstan/phpstan": "^1.10"
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true,
      "symfony/runtime": true
    }
  }
}
